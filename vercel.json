{
  "builds": [
    {
      "src": "dpace/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dpace/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "/vercel/path0"
    }
  }
}